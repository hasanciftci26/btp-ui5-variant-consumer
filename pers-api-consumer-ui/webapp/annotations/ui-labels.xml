<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <!-- OData service must be referenced here and we should give it a namespace -->
    <edmx:Reference Uri="/northwind/northwind.svc/$metadata">
        <edmx:Include Namespace="NorthwindModel" Alias="NorthwindEntityTypes" />
    </edmx:Reference>
    <!-- Common annotation vocabulary reference -->
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Common.v1" Alias="Common" />
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="">
            <Annotations Target="NorthwindEntityTypes.Customer/CustomerID">
                <Annotation Term="Common.Label" String="Customer ID" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/CompanyName">
                <Annotation Term="Common.Label" String="Company Name" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/City">
                <Annotation Term="Common.Label" String="City" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/Country">
                <Annotation Term="Common.Label" String="Country" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/Region">
                <Annotation Term="Common.Label" String="Region" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/ContactName">
                <Annotation Term="Common.Label" String="Contact Name" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/ContactTitle">
                <Annotation Term="Common.Label" String="Contact Title" />
            </Annotations>
            <Annotations Target="NorthwindEntityTypes.Customer/Phone">
                <Annotation Term="Common.Label" String="Phone" />
            </Annotations>          
            <Annotations Target="NorthwindEntityTypes.Region/RegionID">
                <Annotation Term="Common.Label" String="Region ID" />
            </Annotations>          
            <Annotations Target="NorthwindEntityTypes.Region/RegionDescription">
                <Annotation Term="Common.Label" String="Region" />
            </Annotations>          
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
